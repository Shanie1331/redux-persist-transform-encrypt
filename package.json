{
  "name": "@shanie1331/redux-persist-transform-encrypt",
  "version": "7.0.1",
  "description": "Encrypt your Redux store.",
  "type": "module",
  "main": "lib/index.cjs",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.cjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "prepublishOnly": "pnpm run build",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|md|ts|tsx)\"",
    "build": "tsup src/index.ts --dts --format esm,cjs --out-dir lib --clean",
    "test": "vitest run"
  },
  "files": [
    "lib/"
  ],
  "peerDependencies": {
    "redux": ">3.0.0",
    "redux-persist": "^6.x.x"
  },
  "dependencies": {
    "crypto-js": "^4.2.0",
    "json-stringify-safe": "^5.0.1"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.3",
    "@types/json-stringify-safe": "^5.0.2",
    "@types/node": "^18.15.8",
    "prettier": "^2.8.7",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
    "tsup": "^8.0.1",
    "typescript": "^5.0.2",
    "vitest": "^0.34.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shanie1331/redux-persist-transform-encrypt.git"
  },
  "keywords": [
    "redux",
    "redux-persist",
    "redux-persist-transform",
    "redux-persist-transform-encrypt",
    "encryption"
  ],
  "author": "Shashank Malviya <shashankm.malviya@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shanie1331/redux-persist-transform-encrypt/issues"
  },
  "homepage": "https://github.com/Shanie1331/redux-persist-transform-encrypt#readme"
}
